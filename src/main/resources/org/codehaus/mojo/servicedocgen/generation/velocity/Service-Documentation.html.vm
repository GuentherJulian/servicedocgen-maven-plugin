<body>
  <script type="text/javascript">
  <!--
    function toggle_visibility(id) {
      var e = document.getElementById(id);
      if (e.style.display != 'none') {
        e.style.display = 'none';
      } else {
        e.style.display = 'block';
      }
    }
  //-->
  </script>
  <div id="api_info" class="info">
    <h1>$services.info.title</h1>
    <div class="content">
      <p>$services.info.description</p>
      <div class="info_version">
        Version: <span class="version">$services.info.version</span>
      </div>
      #if ($services.info.contact)
      <div class="info_email">
        Contact: <span class="contact"><a href="mailto:$services.info.contact.email">$services.info.contact.name</a></span>
      </div>
      #end
      #if ($services.info.license)
      <div class="info_license">
        License: <span class="license"><a href="$services.info.license.url">$services.info.license.name</a></span>
      </div>
      #end
    </div>
  </div>
  <div id="resources_container" class="container">
    <ul id="resources">
      #foreach ($service in $services.services)
      <li class="resource active">
        <div class="heading">
          <h2 class="ellipsis">
            <span class="service_name"><a onClick="toggle_visibility('$service.id')">$service.name</a></span>:
            <span class="service_description"><a onClick="toggle_visibility('$service.id')">$service.description</a></span>
          </h2>
        </div>
        <div id="$service.id">
          <div class="content">
            <p>$service.description</p>
            $service.javaByteType.retrievalClass.name
          </div>
          <ul class="endpoints">
            <li class="endpoint">
              <ul class="operations">
                #foreach ($path in $service.paths)
                <li class="$path.httpMethod operation">
                  <div class="heading">
                    <h3>
                      <span class="http_method"><a class="toggleOperation" onclick="toggle_visibility('$path.id')">$path.httpMethod</a></span>
                      <span class="path"><a class="toggleOperation" onclick="toggle_visibility('$path.id')">$service.basePath$path.path</a></span>
                    </h3>
                  </div>
                  <div id="$path.id" class="content" style="display: none">
                    $path.description
                    <h4>Parameters</h4>
                    <span>Content-Type:</span> $display.list($service.consumes)
                    #if ($path.parameters.isEmpty())
                    No parameters
                    #else
                    <table>
                      <thead>
                        <tr>
                          <th>Name</th>
                          <th>Location</th>
                          <th>JavaType</th>
                          <th>JavaScriptType</th>
                          <th>Example</th>
                          <th>Description</th>
                        </tr>
                      </thead>
                      <tbody>
                        #foreach ($parameter in $path.parameters)
                        <tr>
                          <td>$parameter.name</td>
                          <td>$parameter.in</td>
                          <td>$esc.html($parameter.javaTypeString)</td>
                          <td>$parameter.javaScriptType</td>
                          <td>
                            <div class="snippet">
                              <pre><code class="json">$parameter.example</code></pre>
                            </div>
                          </td>
                          <td>$parameter.description</td>
                        </tr>
                        #end
                      </tbody>
                    </table>
                    <h4>Response</h4>
                    <span>Content-Type:</span> $display.list($service.produces)
                    <table>
                      <thead>
                        <tr>
                          <th>HTTP Staus Code</th>
                          <th>Reason</th>
                          <th>JavaType</th>
                          <th>JavaScriptType</th>
                          <th>Example</th>
                          <th>Description</th>
                        </tr>
                      </thead>
                      <tbody>
                        #foreach ($response in $path.responses)
                        <tr>
                          <td>$response.statusCode</td>
                          <td>$response.reason</td>
                          <td>$esc.html($response.javaByteType.toString())</td>
                          <td>$response.javaScriptType</td>
                          <td>
                            <div class="snippet">
                              <pre><code class="json">$response.example</code></pre>
                            </div>
                          </td>
                          <td>$response.description</td>
                        </tr>
                        #end
                      </tbody>
                    </table>
                    #end
                  </div>
                </li>
                #end
              </ul>
            </li>
          </ul>
        </div>
      </li>
      #end
    </ul>
  </div>
</body>
